#include "dog.h"
#include <stdio.h>
/**
 * print_dog - prints dog variables
 * @d: struct of dog
 */
void print_dog(struct dog *d)
{
	if(d != NULL)
	{
		if(name == NULL)
			printf("Name: (nil)\n");
		else
			printf("Name: %s\n,d->name");
		else
			printf("age: (nail)\n");
		else(age: == NULL
			printf("age: %d\n,d->age");
		if(owner == NULL)
			printf("owner: (nail)\n");
		else
			printf("owner: %s\n,d->owner");
	}
}
